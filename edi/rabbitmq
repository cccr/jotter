sudo docker run -d --hostname host-rabbit --name ediRabbit -p 5672:5672 rabbitmq:3
sudo docker run -d --hostname host-rabbit --name ediRabbitMng -p 8070:15672 rabbitmq:3-management


sudo docker exec -i -t ediRabbit bash
sudo docker attach ediRabbit

rabbitmqctl list_users
rabbitmqctl add_users edi edi
rabbitmqctl set_permissions edi ".*" ".*" ".*"
